                        <div class="footer-made">
                           " Made with love for great people "
                        </div>

                    </div>
                    <!-- End Footer Text -->

                 </div>
                 <!-- Top Link -->
                 <div class="local-scroll">
                     <a href="#top" class="link-to-top"><i class="fa fa-caret-up"></i></a>
                 </div>
                 <!-- End Top Link -->

            </footer>
<!-- End Foter -->
<!-- ./ all content-->
    </div>
    <!-- scripts-->
    <script src="/javascripts/jquery.min.js"></script>
    <script src="/javascripts/bootstrap.min.js"></script>
    <script src="/javascripts/slick.min.js"></script>
    <script src="/javascripts/main.min.js"></script>
    <script type="text/javascript">
      $(".CBtn").click(function(){
          $('.CBtn').addClass('active');
          $('.ABtn').removeClass('active');
          $('.MBtn').removeClass('active');
          $('#Complains').show();
          $('#Accounts').hide();
          $('#Members').hide();
      });
      $(".ABtn").click(function(){
          $('.CBtn').removeClass('active');
          $('.ABtn').addClass('active');
          $('.MBtn').removeClass('active');
          $('#Complains').hide();
          $('#Accounts').show();
          $('#Members').hide();
      });
      $(".MBtn").click(function(){
          $('.CBtn').removeClass('active');
          $('.ABtn').removeClass('active');
          $('.MBtn').addClass('active');
          $('#Complains').hide();
          $('#Accounts').hide();
          $('#Members').show();
      });

      function CreateMember(){
        let nam = document.getElementById('memberName').value;
        let categor = document.getElementById('memberCategory').value;
        let emailid = document.getElementById('memberEmail').value;
        let mobileNo = document.getElementById('memberMobile').value;
        let pincde = document.getElementById('memberPincode').value;
        let addres = document.getElementById('memberAddress1').value+" "+document.getElementById('memberAddress2').value+" "+ document.getElementById('memberAddress3').value;
        let cit = document.getElementById('memberCity').value;
        let websit = document.getElementById('memberWebsite').value;

        $.post( "/details",{name: nam,email : emailid, mobile  : mobileNo, address  : addres, city  : cit, pincode  : pincde, website  : websit, category : categor})
        .done(function( data ) {
          document.getElementById('memberName').value = "";
          alert("Added Successfully!");
          window.location.href = "http://localhost:3000/members";
        });
      }

      function addComplain(){
        let nam = document.getElementById('clientId').value;
        let categor = document.getElementById('complainCategory').value;
        let Titl = document.getElementById('Title').value;
        let Dat = document.getElementById('Date').value;
        let detail = document.getElementById('Details').value;

        $.post( "/dashboard",{name: nam, category : categor, Title : Titl, Date : Dat, Details: detail})
        .done(function( data ) {
          alert("Added Successfully!");
          window.location.href = "http://localhost:3000/dashboard";
        });
      }
    </script>
        <script type="text/javascript">
       function preventBack(){window.history.forward();}
        setTimeout("preventBack()", 0);
        window.onunload=function(){null};
    </script>

    <script>
$(document).ready(function(){
    var name,email,mobile,address;

    $("#EButton").click(function() {
        name    = $("#memberName").val();
        email   = $("#memberEmail").val();
        mobile  = $("#memberMobile").val();
        address = $("#memberAddress1").val()+", "+$("#memberAddress2").val()+", "+$("#memberAddress3").val()+", "+$("#memberCity").val()+", "+$("#memberPincode").val();

        $.get("/task-create/sendmail",
            {
                name: name,
                email: email,
                mobile: mobile,
                address: address
            },
        function(data) {
        if(data == "sent") {
            alert("Email Sent")
        }
      });
    });
});
</script>
    <!-- ./ scripts-->
  </body>
</html>
