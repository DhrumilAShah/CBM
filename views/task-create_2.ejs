  <body>
    <div class="all">
      <!-- header-->
      <div class="header header-fixed">
        <div class="header-inner">
          <div class="header-logo">
            <a href="dashboard" class="logo">
              <img src="/images/logo-white.png" alt=""/>
            </a>
          </div>
          <div  class="inner-nav desktop-nav nav">
            <ul class="clearlist scroll-nav local-scroll">
            <li class="active"><a href="/dashboard">Complain</a></li>
            <li><a href="/accounts">Accounts</a></li>
            <li><a href="/members">Members</a></li>
            <li><a href="/dashboard/logout">Logout</a></li>
            </ul>
          </div>
        </div>
      </div>
      <!-- ./ header-->
      <!-- content-->
      <div class="page page-dashboard page1">
        <div class="page-nav page-nav-fixed">
          <div class="page-nav-inner">
            <ul class="page-nav__list">
              <li class="active CBtn">
                <a href="#Complains">Add Complains</a>
               </li>
              <li class="ABtn">
                <a href="#Accounts">Update Accounts</a>
              </li>
              <li class="MBtn">
                <a href="#Members">Add Member</a>
              </li>
            </ul>
            <div class="btn-group">

            </div>
          </div>
        </div>
        <section id="Complains">
          <div class="page-header">
            <div class="container-fluid">
              <h1>New Complain</h1>
            </div>
          </div>
          <div class="page-content">
            <div class="container-fluid">
              <form>
                <div class="row pt-20">
                  <div class="col-md-4">
                    <div class="form-group">
                      <label>Client Name</label>
                      <select class="form-control" id="clientId" required >
                        <option value="">--Select --</option>
                        <% for(var i=0; i < clientName.length; i++) { %>
                        <option value="<%= clientName[i]['id']%>"><%= clientName[i]['name']%></option>
                        <%}%>
                      </select>
                    </div>
                    <div class="form-group">
                      <label>Category</label>
                      <select class="form-control" id="complainCategory" required >
                        <option value="">--Select --</option>
                        <option value="AMC">AMC</option>
                        <option value="Hardware">Hardware</option>
                        <option value="Networking">Networking</option>
                        <option value="Software">Software</option>
                        <option value="FormatingWindows">Formating Windows</option>
                      </select>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group">
                      <label>Title</label>
                      <input type="text" placeholder="Complain Title" class="form-control" id="Title" required >
                    </div>
                    <div class="form-group">
                      <label>Date</label>
                      <input type="Date" placeholder="Date" class="form-control" id="Date" required >
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group">
                      <label>Details</label>
                      <textarea rows="3" class="form-control" id="Details" required ></textarea>
                    </div>
                  </div>
                  <div class="col-md-12">
                    <div class="form-group text-center">
                      <button type="submit" class="btn btn-accent" onclick="addComplain()">Add now</button>
                    </div>
                  </div>
                </div>
              </form>
            </div>
          </div>
      </section>
      <section id="Accounts">
          <div class="page-header">
            <div class="container-fluid">
              <h1>New Upadte</h1>
            </div>
          </div>
          <div class="page-content">
            <div class="container-fluid">
              <form action="accounts" method="post" enctype="multipart/form-data">
               <div class="row pt-20">
                 <div class="col-md-4">
                   <div class="form-group">
                     <label>Category</label>
                     <select class="form-control" name="type" required>

                        <option value="">--Select --</option>
                       <option value="bill">Bill</option>
                       <option value="amc">AMC</option>
                     </select>
                   </div>
                 </div>
                 <div class="col-md-4">
                   <div class="form-group">
                     <label>Upload file</label>
                     <input type="file" name="file" placeholder="D:\" class="form-control" required>
                   </div>
                 </div>
                 <div class="col-md-12">
                   <div class="form-group text-center">
                     <button type="submit" class="btn btn-accent">Update</button>
                   </div>
                 </div>
               </div>
             </form>
            </div>
          </div>
      </section>
      <section id="Members">
          <div class="page-header">
            <div class="container-fluid">
              <h1>New Member</h1>
            </div>
          </div>
          <div class="page-content">
            <div class="container-fluid">
              <form action="#" onsubmit="CreateMember()">
                <div class="row pt-20">
                  <div class="col-md-4">
                    <div class="form-group">
                      <label>Name</label>
                      <input type="name" placeholder="Name" class="form-control" id="memberName" required>
                    </div>
                    <div class="form-group">
                      <label>Category</label>
                      <select class="form-control" id="memberCategory" required>
                        <option value="">--Select --</option>
                        <option value="5">Client</option>
                        <option value="4">Engineer</option>
                        <option value="2">Support</option>
                        <option value="3">Account</option>
                      </select>
                    </div>
                    <div class="form-group">
                      <label>Email ID</label>
                      <input type="email" placeholder="Email id" class="form-control" id="memberEmail" required>
                    </div>
                    <div class="form-group">
                      <label>Mobile Number</label>
                      <input type="number" placeholder="Mobile Number" class="form-control" id="memberMobile" required>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group">
                      <label>Address Line 1</label>
                      <input type="name" placeholder="Eg: wing b/603" class="form-control" id="memberAddress1" required>
                    </div>
                    <div class="form-group">
                      <label>Address Line 2</label>
                      <input type="name" placeholder="Eg: Blossom" class="form-control" id="memberAddress2">
                    </div>
                    <div class="form-group">
                      <label>Address Line 3</label>
                      <input type="name" placeholder="Eg: Link Road" class="form-control" id="memberAddress3">
                    </div>
                    <div class="form-group">
                      <label>City</label>
                      <input type="name" placeholder="Eg: Mumbai" class="form-control" default="Mumbai" id="memberCity" required>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group">
                      <label>Pincode</label>
                      <input type="number" placeholder="Eg: 400066" class="form-control" id="memberPincode" required>
                    </div>
                    <div class="form-group">
                      <label>website</label>
                      <input type="name" placeholder="www.cbmindia.com" class="form-control" id="memberWebsite" required>
                    </div>
                  </div>
                    <div class="col-md-12">
                    <div class="form-group text-center">
                      <button type="submit" class="btn btn-accent" id="EButton">Create Now</button>
                    </div>
                  </div>
                </div>
              </form>
            </div>
          </div>
      </section>
    </div>
  <!-- ./ content--><!-- Foter -->
            <footer class="page-section bg-gray-lighter footer pb-30 pt-30">
                <div class="container">

                    <!-- Footer Logo -->
                    <div class="local-scroll mb-10 wow fadeInUp" data-wow-duration="1.5s">
